{
  "name": "tandem",
  "version": "0.4.1",
  "description": "Remote pair programming tool",
  "productName": "Tandem",
  "main": "main.js",
  "build": {
    "appId": "stream.tandem.app",
    "files": [
      "css/",
      "icons/",
      "node_modules/",
      "reception/dist/",
      "displaychampion.html",
      "displaychampion.js",
      "main.js",
      "package.json"
    ],
    "mac": {
      "icon": "icons/app.icns",
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg"
      ]
    }
  },
  "scripts": {
    "postinstall": "electron-rebuild",
    "build": "webpack --display-error-details",
    "start": "electron .",
    "start:dev": "cross-env TANDEM_DEVELOPER_TOOLS=true TANDEM_LOG_LEVEL=debug TANDEM_PRINT_SENSITIVE_VALUES_IN_LOGS=true electron .",
    "start:debug": "cross-env TANDEM_DEVELOPER_TOOLS=true TANDEM_DEBUG_WINDOWS=true TANDEM_LOG_LEVEL=debug TANDEM_PRINT_SENSITIVE_VALUES_IN_LOGS=true TANDEM_VERBOSE_CHROMIUM=true electron .",
    "dist": "electron-builder",
    "test:unit": "tsc -p test --noEmit && mocha -r ts-node/register test/**/*.ts",
    "test:reception": "cd reception && elm test",
    "test": "yarn test:unit && yarn test:reception"
  },
  "repository": "",
  "keywords": [],
  "author": "Adam Neumann",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/mocha": "^2.2.44",
    "@types/sinon": "^4.1.2",
    "@types/sinon-chai": "^2.7.29",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.4",
    "electron": "5.0.9",
    "electron-builder": "21.2.0",
    "electron-rebuild": "^1.8.5",
    "elm": "^0.18.0",
    "elm-test": "^0.18.10",
    "elm-webpack-loader": "^4.3.1",
    "file-loader": "^0.11.2",
    "mocha": "^4.0.1",
    "mock-require": "3.0.1",
    "node-sass": "^4.12.0",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.18.2",
    "ts-loader": "^3.1.1",
    "ts-node": "^4.0.2",
    "typescript": "^2.6.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1",
    "webpack-node-externals": "^1.6.0"
  },
  "resolutions": {
    "**/**/fsevents": "^1.2.9",
    "**/**/ffi": "github:lxe/node-ffi#node-12",
    "**/**/ref": "github:lxe/ref#node-12",
    "**/**/ref-struct": "github:lxe/ref-struct#node-12"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "electron-log": "^2.2.12",
    "fs-extra": "^5.0.0",
    "moment": "^2.19.4",
    "nodobjc": "^2.1.0",
    "robotjs": "octalmage/robotjs#9a3b161e10897a1269cdccfae5e343334915abc2",
    "rxjs": "~5.5.2",
    "simple-peer": "~8.1.1"
  }
}
