{
  "name": "tandem",
  "version": "0.2.1",
  "description": "Developer centric screen sharing tool",
  "productName": "Tandem",
  "main": "main.js",
  "build": {
    "appId": "stream.tandem.app",
    "files": [
      "css/",
      "icons/",
      "node_modules/",
      "reception/dist/",
      "displaychampion.html",
      "displaychampion.js",
      "main.js",
      "package.json"
    ],
    "mac": {
      "icon": "icons/app.icns",
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg"
      ]
    }
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "webpack && electron .",
    "build": "webpack --display-error-details",
    "build:debug": "DEBUG_TOOLS=true webpack --display-error-details",
    "dist": "electron-builder",
    "test:reception": "cd reception && elm test",
    "test": "yarn test:reception"
  },
  "repository": "",
  "keywords": [],
  "author": "Adam Neumann",
  "license": "UNLICENSED",
  "devDependencies": {
    "css-loader": "^0.28.4",
    "electron": "~1.7.8",
    "electron-builder": "^19.45.4",
    "elm": "^0.18.0",
    "elm-test": "^0.18.10",
    "elm-webpack-loader": "^4.3.1",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-loader": "^3.1.1",
    "typescript": "^2.6.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "electron-log": "^2.2.12",
    "fs-extra": "^5.0.0",
    "moment": "^2.19.4",
    "nodobjc": "^2.1.0",
    "robotjs": "~0.4.7",
    "rxjs": "~5.5.2",
    "simple-peer": "~8.1.1"
  }
}
